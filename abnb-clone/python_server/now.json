{
    "version": 2,
    "name": "abnb-server-python",
    "builds": [
        {
            "src": "*.py",
            "use": "@liudonghua123/now-flask",
            "config": {
                "maxLambdaSize": "30mb"
            }
        }
    ],
    "routes": [
        {
            "src": "/.*",
            "dest": "/"
        }
    ]
}